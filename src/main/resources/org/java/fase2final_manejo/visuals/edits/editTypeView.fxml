<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.shape.Line?>
<?import javafx.geometry.Insets?>
<Pane xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.java.fase2final_manejo.controllers.edits.editTypeController"
      stylesheets="@../../stylesheets/typeStyle.css"
      prefHeight="225.0" prefWidth="300.0">
    <children>
        <Pane layoutX="15.0" layoutY="15.0" prefHeight="195.0" prefWidth="270.0" styleClass="mainPane">
            <children>
                <Button fx:id="btnCerrar" layoutX="230.0" layoutY="15.0" mnemonicParsing="false" onAction="#cerrar" prefHeight="24.0" prefWidth="22.0" styleClass="btnSalir">
                    <graphic>
                        <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../images/x.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
                <Label layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="190.0" styleClass="txtTitulo" text="Editar tipo existente" textAlignment="JUSTIFY" />
                <VBox layoutY="42.0" prefHeight="0.0" prefWidth="271.0">
                    <children>
                        <Line endX="-150.0" layoutY="10.0" startX="100.0" stroke="#f2f2f2">
                            <VBox.margin>
                                <Insets left="10.0" />
                            </VBox.margin>
                        </Line>
                    </children>
                </VBox>
                <VBox layoutX="11.0" layoutY="48.0" prefHeight="106.0" prefWidth="251.0">
                    <children>
                        <Region prefHeight="8.0" prefWidth="251.0" />
                        <Label styleClass="txtNormal" text="Nombre del tipo" />
                        <TextField fx:id="txtTipo" promptText="Nombre" />
                        <Region prefHeight="10.0" prefWidth="251.0" />
                        <Label styleClass="txtNormal" text="Año" />
                        <TextField fx:id="txtAnio" promptText="Año" />
                    </children>
                </VBox>
                <Button fx:id="btnAñadir" layoutX="104.0" layoutY="157.0" mnemonicParsing="false" onAction="#editarTipo" styleClass="btnNuevo" text="Editar">
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
            </children>
        </Pane>
    </children>
</Pane>
